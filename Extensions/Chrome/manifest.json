{
    "name": "Fonality Extension"
    , "version": "2.1.6"
    , "description": "Extension for HUDWeb"
    , "manifest_version": 2
    , "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
    , "permissions": [

        "tabs"
        , "https://*.fonality.com/*"
		, "https://*.fon9.com/*"
		, "https://localhost/*"
		, "http://localhost/*"
    ]
    , "icons": {
        "48":"img/logo-48.png"
        , "128":"img/logo-128.png"
    }
    , "background": {
        "page": "bg.html"
    }
    , "content_scripts":[{
        "matches": [
            "https://*.fonality.com/*"
			, "https://*.fon9.com/*"
            , "https://localhost/*"
            , "http://localhost/*" ]
        , "js": ["fj-script.js"]
	}]
}